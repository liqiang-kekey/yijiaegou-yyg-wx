<view class="wrap">
  <view class="cate-list" wx:for="{{rushList}}" wx:key="id" wx:if="{{rushList.length}}">
    <image class='topic-img' src="{{item.cate_info.banner}}" mode="widthFix" wx:if="{{index==0&&item.cate_info.banner}}"></image>
    <view class="cate-title">
      <image class='title-img' src="{{item.cate_info.logo}}" mode="widthFix" wx:if="{{item.cate_info.logo}}"></image>
    </view>
        <!--一行一列小图样式-->
    <view wx:if="{{is_show_prolayout==1}}">
      <view class="list3">
        <i-rush-spu class="item" wx:for="{{item.list}}" wx:if="{{item.list.length}}" wx:for-index="idx" wx:key="idx" wx:for-item="goodsItem" spuItem="{{goodsItem}}" reduction="{{reduction}}" stopClick="{{stopClick}}" bind:openSku="openSku" needAuth="{{needAuth}}" bind:authModal="authModal" bind:changeCartNum="changeCartNum" width250="{{true}}" bind:vipModal="vipModal"></i-rush-spu>
      </view>
    </view>
    <!--一行一列小图样式--end-->
    <!--一行一列大图样式-->
    <view wx:if="{{is_show_prolayout==2}}">
      <view class="list2">
        <i-rush-spu-big class="item" wx:for="{{item.list}}" wx:if="{{item.list.length}}" wx:for-index="idx" wx:key="idx" wx:for-item="goodsItem" spuItem="{{goodsItem}}" reduction="{{reduction}}" stopClick="{{stopClick}}" bind:openSku="openSku" needAuth="{{needAuth}}" bind:authModal="authModal" bind:changeCartNum="changeCartNum" width250="{{true}}" bind:vipModal="vipModal"></i-rush-spu-big>
      </view>
    </view>
    <!--一行一列大图样式--end-->
    <!--一行两列样式-->
    <view wx:if="{{is_show_prolayout==3}}">
      <view class="list">
        <i-rush-spu-1-2 class="item" wx:for="{{item.list}}" wx:if="{{item.list.length}}" wx:for-index="idx" wx:key="idx" wx:for-item="goodsItem" spuItem="{{goodsItem}}" reduction="{{reduction}}" stopClick="{{stopClick}}" bind:openSku="openSku" needAuth="{{needAuth}}" bind:authModal="authModal" bind:changeCartNum="changeCartNum" width250="{{false}}" bind:vipModal="vipModal"></i-rush-spu-1-2>
      </view>
    </view>
    <!--一行三列样式-->
    <view wx:if="{{is_show_prolayout==4}}">
      <view class="list">
        <i-rush-spu-col class="item" wx:for="{{item.list}}" wx:if="{{item.list.length}}" wx:for-index="idx" wx:key="idx" wx:for-item="goodsItem" spuItem="{{goodsItem}}" reduction="{{reduction}}" stopClick="{{stopClick}}" bind:openSku="openSku" needAuth="{{needAuth}}" bind:authModal="authModal" bind:changeCartNum="changeCartNum" width250="{{true}}" bind:vipModal="vipModal"></i-rush-spu-col>
      </view>
    </view>
    <!--猜你喜欢内容-->
    <view wx:if="{{false}}">
      <view class="list3">
        <i-one-two-spu class="item" wx:for="{{item.list}}" wx:if="{{item.list.length}}" wx:for-index="idx" wx:key="idx" wx:for-item="goodsItem" spuItem="{{goodsItem}}" reduction="{{reduction}}" stopClick="{{stopClick}}" bind:openSku="openSku" needAuth="{{needAuth}}" bind:authModal="authModal" bind:changeCartNum="changeCartNum" width250="{{true}}" bind:vipModal="vipModal"></i-one-two-spu>
      </view>
    </view>
    <!---->
    <i-load-more iClass="loadMore" loading="{{loadMore}}" tip="{{loadText}}" wx:if="{{!loadOver&&!showEmpty&&loadMore}}"></i-load-more>
  </view>
  <view class="none-rush-list" wx:if="{{showEmpty}}">
    <image class="img-block" src="../../images/icon-index-empty.png"></image>
    <view class="h1">暂时没有团购</view>
    <view class="h2">我们正在为您准备更优惠的团购</view>
  </view>
  <i-cart-btn cartNum="{{cartNum}}" iClass="{{is_show_cate_tabbar==1?'cartBtn':''}}" wx:if="{{is_show_cate_tabbar!=1}}"></i-cart-btn>
  <i-aside-btn showShare="{{true}}" showHome="{{is_show_cate_tabbar!=1}}" showContact="{{is_show_cate_tabbar==1}}"></i-aside-btn>
  <i-tabbar currentIdx='-1' cartNum='{{cartNum}}' class="tabbar {{isIpx?'pb20':''}}" wx:if="{{is_show_cate_tabbar==1}}" />
</view>

<i-new-auth needAuth="{{needAuth&&showAuthModal}}" bind:authSuccess="authSuccess" bind:cancel="authModal"></i-new-auth>
<sku visible="{{visible}}" skuList="{{skuList}}" cur_sku_arr="{{cur_sku_arr}}" bind:cancel="closeSku" sku_val="{{sku_val}}" sku="{{sku}}" goodsid="{{addCar_goodsid}}" bind:changeCartNum="changeCartNum" bind:vipModal="vipModal"></sku>

<i-vip-modal visible="{{showVipModal}}" imgUrl="{{pop_vipmember_buyimage}}" />
